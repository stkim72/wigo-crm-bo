<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<grid>
    <meta>
        <deletable value="N"/>
        <rollbackable value="N"/>
        <appendable value="N"/>
        <readOnly value="N"/>
        <showCheckbox value="N"/>
        <showNumber value="Y"/>
        <showStateBar value="N"/>
        <showHeaderSum value="N"/>
